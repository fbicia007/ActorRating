.search-container {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  background-color: #42bd56;
  color: #aaa;
  height: 80rpx;
  padding: 10rpx 0rpx;
  z-index: 100;
}

.search-container input {
  background: #fff;
  color: #aaa;
  margin-left: 20rpx;
  padding: 10rpx 20rpx;
  height: 40rpx;
  width: 85%;
  border-radius: 8rpx;
}

.clearButtonView {
  width: 15%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.clearButtonText {
  color: #fff;
}

.test1 {
  width: 100%;
  height: 100%;
  background-color: red;
}

.test {
  background-color: rebeccapurple
}

.item {
  display: flex;
  padding: 10rpx 10rpx;
  border-bottom: 2rpx solid #eee;
  align-items: center;
}

.actor-photo {
  width: 120rpx;
  height: 120rpx;
  margin-right: 20rpx;
}

.actor-name {
  width: 60%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.star {
  width: 30rpx;
  height: 30rpx;
  src: "../../images/normal.png";
}

.section-item-header {
  display: flex;
  align-items: center;
  font-size: 26rpx;
  color: #a8a8a8;
  background-color: #f2f4f5;
  padding: 4rpx 20rpx;
}

.hover{
  background-color: #F2F2F2;
}
